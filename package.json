    "lodash": "^4.8.2",
    "yosay": "^1.0.2"
  },
  "devDependencies": {
    "coveralls": "^2.11.8",
    "eslint": "^2.1.0",
    "eslint-config-xo-space": "^0.10.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "yeoman-assert": "^2.0.0",
    "yeoman-test": "^1.0.0"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "mocha": true
    }
  },
  "repository": "https://github.com/tcdl/generator-msb.git",
  "scripts": {
    "cs": "eslint generators/**/*.js test/**",

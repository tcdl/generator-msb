{
  "name": "generator-msb",
  "version": "0.1.0",
  "description": "Generate a microservice with MSB support",
  "homepage": "https://github.com/tcdl/generator-msb",
  "author": {
    "name": "Nikita Galkin",
    "email": "nikita@galk.in"
  },
  "files": [
    "generators"
